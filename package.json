{
  "name": "rocketjump-core",
  "sideEffects": false,
  "version": "3.1.1",
  "description": "The core of rocketjump",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.es.js",
  "repository": "https://github.com/inmagik/rocketjump-core",
  "authors": [
    "Giovanni Fumagalli <giovanni.fumagalli@inmagik.com>",
    "Mauro Bianchi <mauro.bianchi@inmagik.com>",
    "Alberto Osio <osioalberto@gmail.com>"
  ],
  "scripts": {
    "format": "prettier --no-semi --single-quote --trailing-comma es5 --write \"{src,__{tests,mocks}__}/**/*.js\"",
    "prebuild": "rimraf lib",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inmagik/rocketjump-core/issues"
  },
